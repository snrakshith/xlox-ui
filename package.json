{
  "name": "@xlox/ui",
  "version": "0.0.1",
  "description": "UIkit components, library based on IKURE's design system",
  "homepage": "http://snrakshith.github.io/xloxui",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/**"
  ],
  "exports": {
    ".": {
      "types": "./lib/components/index.d.ts",
      "import": "./lib/components/index.js",
      "require": "./lib/components/index.mjs"
    },
    "./components": {
      "types": "./lib/components/index.d.ts",
      "import": "./lib/components/index.js",
      "require": "./lib/components/index.mjs"
    },
    "./primitives": {
      "types": "./lib/primitives/index.d.ts",
      "import": "./lib/primitives/index.js",
      "require": "./lib/primitives/index.mjs"
    }
  },
  "private": true,
  "scripts": {
    "dev": "npm run clean && tsup --watch",
    "build": "npm run clean && tsup",
    "experiment": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rimraf lib",
    "sb": "storybook dev -p 6006 --disable-telemetry",
    "crash-reports": "storybook dev -p 6006 --enable-crash-reports",
    "build-storybook": "storybook build -o dist/storybook"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "styled-components": "^6.0.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@storybook/addon-docs": "^7.4.1",
    "@storybook/addon-essentials": "7.4.1",
    "@storybook/addon-interactions": "7.4.1",
    "@storybook/addon-links": "7.4.1",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/addon-viewport": "^7.4.1",
    "@storybook/blocks": "7.4.1",
    "@storybook/manager-api": "^7.4.1",
    "@storybook/react": "7.4.1",
    "@storybook/react-webpack5": "7.4.1",
    "@storybook/testing-library": "0.2.0",
    "@storybook/theming": "^7.4.1",
    "@swc/core": "^1.3.84",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.27",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-preval": "^5.1.0",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^8.46.0",
    "eslint-plugin-storybook": "^0.6.13",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "5.0.0",
    "storybook": "7.4.1",
    "styled-components": "^6.0.8",
    "tsup": "7.2.0",
    "typescript": "^5.1.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "clsx": "^2.0.0"
  }
}
